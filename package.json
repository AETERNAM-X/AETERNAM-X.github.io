{
  "name": "aeternam-x.github.io-main",
  "version": "1.0.0",
  "description": "Optimized Jekyll website.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "sudo apt-get update && sudo apt-get install -y libx11-xcb1 libxcomposite1 libxrandr2 libxtst6 libnss3 libasound2 libgbm-dev",
    "clean": "bundle exec jekyll clean --config _config.yml",
    "build:jekyll": "bundle exec jekyll build --config _config.yml",
    "optimize:css": "node optimize-css.js",
    "minify:html": "node minify.js",
    "minify:js": "node minify-js.js",
    "critical:css": "node critical-css.mjs",
    "build": "npm run clean && npm run build:jekyll && npm run optimize:css && npm run critical:css && npm run minify:html && npm run minify:js"
  },
  "keywords": [
    "jekyll",
    "optimization",
    "performance"
  ],
  "author": "AETERNAM-X",
  "license": "ISC",
  "devDependencies": {
    "cssnano": "^7.0.7",
    "htmlnano": "^2.1.2",
    "lightningcss": "^1.30.1",
    "postcss": "^8.5.5",
    "purgecss": "^7.0.2",
    "svgo": "^3.3.2",
    "terser": "^5.42.0",
    "critical": "^7.2.1"
  }
}